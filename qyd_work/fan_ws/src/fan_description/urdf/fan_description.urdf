<?xml version="1.0" encoding="UTF-8" ?>
<robot name="fan">
    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <material name="blue">
                <color rgba="0 0 .8 1"/>
            </material>
        </visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <collision>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
        </collision>
        <inertial>
            <origin xyz="0 0 0" />
            <mass value="100" />
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
        </inertial>
    </link>
    <link name="wall">
        <visual>
            <geometry>
                <box size="10 1 10"/>
            </geometry>
            <material name="blue">
                <color rgba="0 0 .8 1"/>
            </material>
        </visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <collision>
            <geometry>
                <box size="10 1 10"/>
            </geometry>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
        </collision>
        <inertial>
            <origin xyz="0 0 0" />
            <mass value="20" />
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
        </inertial>
    </link>
    <link name="joint">
        <visual>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <collision>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
        </collision>
        <inertial>
            <origin xyz="0 0 0" />
            <mass value="0.1" />
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
        </inertial>
    </link>
    <link name="pole">
        <visual>
            <geometry>
                <box size="1 1 8"/>
            </geometry>
            <material name="blue">
                <color rgba="0 0 .8 1"/>
            </material>
        </visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <collision>
            <geometry>
                <box size="1 1 8"/>
            </geometry>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
        </collision>
        <inertial>
            <origin xyz="0 0 0" />
            <mass value="1" />
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
        </inertial>
    </link>
    <joint name="wall2base_link" type="fixed">
        <parent link="base_link"/>
        <child link="wall"/>
        <origin xyz="0 0 5" rpy="0 0 0"/>
    </joint>
    <joint name="joint2wall" type="fixed">
        <parent link="wall"/>
        <child link="joint"/>
        <origin xyz="0 -1 0" rpy="0 0 0"/>
    </joint>
    <joint name="pole2joint" type="fixed">
        <parent link="joint"/>
        <child link="pole"/>
        <origin xyz="0 -1 0" rpy="0 0 0"/>
    </joint>
</robot>
